pipeline{

  agent {label 'DES-BNEXT-152-etorres'}

  stages{
    stage('py-exe'){
      steps{
          script{
            def getIpAddresses = bat(script: 'python C:\\Users\\etorres\\PycharmProjects\\IpAddresses\\main.py', returnStdout: true).trim()
            def ips = getIpAddresses.split(',')
            env.IPS_TO_PING = ips.join(',')
          }
      }
    }
  }
}
